{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/ShoppyS logo .png",
      "dest": "/ShoppyS logo .png"
    },
    {
      "src": "/ShoppyS logo bag.png",
      "dest": "/ShoppyS logo bag.png"
    },
    {
      "src": "/ShoppyS logo white .png",
      "dest": "/ShoppyS logo white .png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/assets/(.*\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ],
  "regions": ["sin1"]
}
